class OverloadDemo
{
    void add(int a)
    {
        System.out.println(a+a);
    }
    void add(int a, int b)
    {
    System.out.println(a+b);
    }
    void add(float a, float b)
    {
    System.out.println(a+b);
    }
    public static void main(String args[]) 
    {
    OverloadDemo p= new OverloadDemo();
        p.add(10);
        p.add(10,20);
        p.add(4.5f,5.5f);
    }
}
