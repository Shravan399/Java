//Example for arguments
class Demo3 {
    void add(int a, int b)
    {
        int c;
        c=a+b;
        System.out.println(c);
    }
    public static void main(String[] args) 
    {
        Demo3 p=new Demo3();//classname obj name=new classname
        p.add(100,200);
        Demo3 q=new Demo3();//classname obj name=new classname
        q.add(12,15);
       
    }
}
